{
  "name": "server",
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "dev": "NODE_ENV=development nodemon --exec 'node --experimental-specifier-resolution=node --loader ts-node/esm' server.ts",
    "dev2": "nodemon --watch './**/*.ts' --exec 'node --experimental-specifier-resolution=node --loader ts-node/esm' server.ts",
    "dev3": "tsc-watch --onSuccess \"yarn run dev\"",

    "start": "node build/server.js",
    "build": "tsc -p ."
  },
  "dependencies": {
    "@types/node": "^17.0.8",
    "express": "^4.17.2",
    "jose": "^4.3.8",
    "node-fetch": "^3.1.1",
    "openid-client": "^5.1.2",
    "tsc-watch": "^4.6.0",
    "typescript": "^4.5.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0"
  }
}
